@model HazarSchool.ViewModels.ResponseModels.StudentResponseViewModels.StudentsResponseModel

<style>
    a:hover {
        font-size: 80%;
    }
</style>

<h1 style="margin-top: 50px; margin-bottom:30px;">List of Female Students</h1>
<table class="table table-sm table-hover">
    <thead>
        <tr>
            <th style="font-weight: bolder; font-size: larger;">
                S/N
            </th>
            <th>
                Firt Name
            </th>
            <th>
                Last Name
            </th>
            <th>
               Email
            </th>
  </thead>
  <tbody>
        @foreach (var student in Model.Data)
        {
            <tr>
                <td>
                    @student.Id
                </td>
                <td>
                    @student.FirstName
                </td>
                <td>
                    @student.LastName
                </td>
                <td>
                    @student.Email
                </td>
            </tr>
        }
 </tbody>
 </table>

